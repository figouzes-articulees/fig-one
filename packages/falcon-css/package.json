{
  "name": "@figouzes/falcon-css",
  "description": "Typesafe CSS composer",
  "version": "0.1.12",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "https://github.com/figouzes-articulees/fig-one/tree/main/packages/falcon-css",
  "keywords": [
    "css",
    "typescript",
    "agnostic"
  ],
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "tsc -p ./tsconfig.json --outDir dist && tscpaths -p ./tsconfig.json -s ./src -o ./dist",
    "prepare": "yarn run build"
  },
  "dependencies": {},
  "devDependencies": {
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": "14"
  }
}
